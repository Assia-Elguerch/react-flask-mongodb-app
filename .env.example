# MongoDB Credentials
# Copy this file to .env and update with your actual credentials
# DO NOT commit .env to version control

MONGO_INITDB_ROOT_USERNAME=your_username_here
MONGO_INITDB_ROOT_PASSWORD=your_password_here
MONGO_INITDB_DATABASE=flaskdb

# MongoDB Connection (for API)
MONGODB_HOST=mongodb
MONGODB_USERNAME=your_username_here
MONGODB_PASSWORD=your_password_here
MONGODB_DATABASE=flaskdb

# JWT Secret Key (generate a strong random key in production)
# You can generate one using: python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET_KEY=secret_key

